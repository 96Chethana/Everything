// Hotel management system administration 

let ifPersonOneVegan = true;
let ifPersonTwoVegan = false;


if (ifPersonOneVegan && ifPersonTwoVegan) { //If both are Vegan
    console.log("offer only vegan food")
}else if (ifPersonOneVegan || ifPersonTwoVegan) {// If one of them is vegan 
    console.log("Main course to be Vegan and Non vegan fod as Side Dishes");
}else {// If None Of Them Were Vegan
    console.log("Offer any thing on the Menu");
}
// ends


